{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Bebas Neue.ttf",
            "./assets/fonts/Roboto/Roboto-Black.tff",
            "./assets/fonts/Roboto/Roboto-BlackItalic.tff",
            "./assets/fonts/Roboto/Roboto-Bold.tff",
            "./assets/fonts/Roboto/Roboto-BoldItalic.tff",
            "./assets/fonts/Roboto/Roboto-Italic.tff",
            "./assets/fonts/Roboto/Roboto-Light.tff",
            "./assets/fonts/Roboto/Roboto-LightItalic.tff",
            "./assets/fonts/Roboto/Roboto-Medium.tff",
            "./assets/fonts/Roboto/Roboto-MediumItalic.tff",
            "./assets/fonts/Roboto/Roboto-Regular.tff",
            "./assets/fonts/Roboto/Roboto-Thin.tff",
            "./assets/fonts/Roboto/Roboto-ThinItalic.tff"
          ]
        }
      ],
      "expo-video"
    ],
    "name": "mecha-mayhem-app",
    "slug": "mecha-mayhem-app",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F85050"
    },
    "icon": "./assets/icons/icon.png",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "backgroundColor": "#F85050"
  }
}
